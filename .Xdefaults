xterm*font:	-*-fixed-*-*-*-*-10-*-*-*-*-*-*-*
xterm*boldFont:	-*-fixed-bold-*-*-*-10-*-*-*-*-*-*-*
URxvt.matcher.pattern.1:	\\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt.matcher.button:	3
URxvt.perl-ext-common:	default,matcher,font-size
URxvt.font-size.step: 4
URxvt.url-launcher: chromium
URxvt.visualBell:	true
URxvt.urgentOnBell: true
URxvt.scrollBar:	false
#URxvt*font: xft:fixed:pixelsize=9
URxvt*font: xft:Dejavu Sans Mono for Powerline:pixelsize=11:antialias=true:autohint=false,xft:Symbola:pixelsize=10
#URxvt*boldFont: xft:fixed:pixelsize=9
URxvt.searchable-scrollback:	CM-s
URxvt.blink:	true
URxvt.geometry:	75x27
URxvt.internalBorder: 2

URxvt.keysym.C-Up:     perl:font-size:increase
URxvt.keysym.C-Down:   perl:font-size:decrease

