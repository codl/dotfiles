"pavucontrol"
  Alt + Mod4 + v

"amixer -c 0 sset Master 2%-"
  Mod4 + b:5
"amixer -c 0 sset Master 2%+"
  Mod4 + b:4

"sscrot"
  Print
"sscrot -s"
  shift + Print
"sscrot -v"
  Control + Print

"slock"
  Control+Shift + Escape

"true"
    F16
"true"
    b:8
"true"
    b:9

"mpc prev"
  Mod4 + F10
"mpc toggle"
  Mod4 + F11
"mpc next"
  Mod4 + F12


"setxkbmap fr -option compose:menu; xmodmap $HOME/.xmodmap"
  Mod4 + F9
"setxkbmap us"
  Mod4 + F8
